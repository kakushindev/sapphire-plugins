{
    "name": "@kakushin/pino-logger",
    "version": "1.1.1",
    "description": "Plugins for @sapphire/framework to have pino logger.",
    "homepage": "https://github.com/kakushindev/sapphire-plugins/tree/main/packages/pino-logger",
    "bugs": {
        "url": "https://github.com/kakushindev/sapphire-plugins/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kakushindev/sapphire-plugins.git",
        "directory": "packages/command-context"
    },
    "license": "AGPL-3.0",
    "author": "Kakushin Devs <hello@kakushin.dev>",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./register": {
            "import": "./register.mjs",
            "require": "./register.js",
            "types": "./register.d.ts"
        }
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "scripts": {
        "build": "rimraf dist && tsc -b src && rollup -c rollup.bundle.mjs && npm run esm:default && npm run esm:register",
        "esm:default": "gen-esm-wrapper dist/index.js dist/index.mjs",
        "esm:register": "gen-esm-wrapper dist/register.js dist/register.mjs",
        "format": "prettier --write src/**/*.ts",
        "lint": "eslint src --ext ts",
        "lint:fix": "eslint src --fix --ext ts",
        "prepareForRelease": "npm run build && npm run format && npm run lint && npm run lint:fix",
        "release": "npm publish --access public"
    },
    "eslintConfig": {
        "parserOptions": {
            "project": "./tsconfig.eslint.json"
        },
        "extends": "@hazmi35/eslint-config/typescript",
        "ignorePatterns": [
            "dist/*",
            "jest.config.ts",
            "rollup.bundle.ts"
        ]
    },
    "dependencies": {
        "@sapphire/framework": "^4.8.2",
        "discord.js": "^14.14.1",
        "pino": "^8.16.2",
        "tslib": "^2.6.2"
    },
    "devDependencies": {
        "@hazmi35/eslint-config": "^11.0.0",
        "@jest/types": "^29.6.3",
        "@sapphire/decorators": "^6.0.2",
        "@sapphire/prettier-config": "2.0.0",
        "@typescript-eslint/eslint-plugin": "^6.11.0",
        "@typescript-eslint/parser": "^6.11.0",
        "eslint": "8.53.0",
        "gen-esm-wrapper": "^1.1.3",
        "pino-pretty": "^10.2.3",
        "prettier": "^3.1.0",
        "rimraf": "^5.0.5",
        "rollup": "^4.4.0",
        "rollup-plugin-version-injector": "^1.3.3",
        "typescript": "5.2.2"
    },
    "engines": {
        "node": ">=16.6.0",
        "npm": ">=7.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "readme": "https://github.com/kakushindev/sapphire-plugins/blob/main/README.md"
}
